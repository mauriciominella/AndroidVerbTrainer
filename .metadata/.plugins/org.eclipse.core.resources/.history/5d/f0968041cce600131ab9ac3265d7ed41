package com.bravi.capigoatverbtrainer.domain;

public class Verb {
	
	public String infinitive;
	public String simple;
	public String participle;
	
	public Verb(String _infinitive, String _simple, String _participle){
		this.infinitive = _infinitive;
		this.simple = _simple;
		this._participle = _participle;
	}
	
}
